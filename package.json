{
  "$schema": "https://json.schemastore.org/package",
  "name": "@skilbjo/config-rc",
  "version": "0.0.1",
  "description": "eslint, prettier, & tsconfig config",
  "main": "index.js",
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skilbjo/config-rc.git"
  },
  "scripts": {
    "prepare": "if [[ -z $GITHUB_ACTIONS ]]; then npx husky install; fi",
    "eslint": "eslint .",
    "lint": "npm run eslint",
    "depcheck": "depcheck",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "eslint",
    "eslintrc"
  ],
  "author": "skilbjo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skilbjo/config-rc/issues"
  },
  "homepage": "https://github.com/skilbjo/config-rc#readme",
  "dependencies": {
    "@tsconfig/node22": "22.0.2",
    "@types/jest": "30.0.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-typescript": "3.0.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "28.14.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-prettier": "5.5.3",
    "eslint-plugin-security": "3.0.1",
    "prettier": "3.6.2",
    "typescript": "5.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-angular": "16.2.4",
    "@types/node": "24.0.13",
    "depcheck": "1.4.7",
    "husky": "8.0.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
